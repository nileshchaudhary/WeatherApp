<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primaryDarkColor_700</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor_700</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>

    </style>
    <style name="ActionBarPopupThemeOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="Toolbartitle" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        
        <item name="android:textColor">@android:color/white</item>
        
    </style>
    <style name="ToolbarSubtitle" parent="TextAppearance.Widget.AppCompat.Toolbar.Subtitle">
        <item name="android:textColor">56FFFFFF</item>


    </style>

</resources>
